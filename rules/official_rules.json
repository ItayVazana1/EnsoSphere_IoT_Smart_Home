    {
      "id": "autumn_rule_0600",
      "category": "occupant_action",
      "description": "Hallway motion in the dark triggers lights",
      "conditions_logic": "AND",
      "conditions": [
        {
          "type": "sensor",
          "sensor_id": "motion_hallway",
          "operator": "==",
          "value": "detected"
        },
        {
          "type": "environment",
          "parameter": "is_daytime",
          "operator": "==",
          "value": false
        }
      ],
      "actions": [
        {
          "device_id": "lights_hallway",
          "command": "turn_on",
          "parameters": {}
        }
      ]
    },
